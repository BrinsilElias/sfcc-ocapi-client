{
  "env": {
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          { "targets": { "browsers": "> 0.25%, not dead" } }
        ]
      ],
      "plugins": [["@babel/plugin-transform-runtime", { "corejs": 3 }]]
    },
    "node": {
      "presets": [["@babel/preset-env", { "targets": { "node": "current" } }]],
      "plugins": [["@babel/plugin-transform-runtime", { "corejs": 3 }]]
    }
  }
}
